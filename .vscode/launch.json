  "version": "0.2.0",
  "configurations": [
    {
      "name": "Ramesh Cloth House Dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart"
    },
    {
      "name": "Ramesh Cloth House Dev for web",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
      ]
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "runtimeArgs": ["chrome --disable-web-security"]
    }
  ]
}
